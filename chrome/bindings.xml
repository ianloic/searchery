<?xml version="1.0"?>

<bindings id="searcheryBindings"
          xmlns="http://www.mozilla.org/xbl"
          xmlns:html="http://www.w3.org/1999/xhtml"
          xmlns:xul="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
          xmlns:xbl="http://www.mozilla.org/xbl">

  <binding id="autocomplete-rich-result-popup"
           extends="chrome://browser/content/urlbarBindings.xml#urlbar-rich-result-popup">
    <content ignorekeys="true">
      <xul:label class="searchery-header" value="History and Bookmarks" />
      <xul:richlistbox anonid="richlistbox" class="autocomplete-richlistbox" flex="1"/>
      <xul:label class="searchery-header" value="Search Engines" />
    </content>

    <implementation implements="nsIAutoCompletePopup">
      <constructor><![CDATA[
        dump('hello, world\n');
      ]]></constructor>
    </implementation>
  </binding>
</bindings>

